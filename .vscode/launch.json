{
  "configurations": [

      {
          "name": "Build",
          "type": "cppdbg",
          "request": "launch",
          "program": "${fileDirname}/cot",
          "args": [
              "127.0.0.1",
              "59000",
              "193.136.138.142",
              "59000"
          ],
          "stopAtEntry": true,
          "cwd": "${fileDirname}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              },
              {
                  "description": "Set Disassembly Flavor to Intel",
                  "text": "-gdb-set disassembly-flavor intel",
                  "ignoreFailures": true
              }
          ],
          "preLaunchTask": "Build",
          "miDebuggerPath": "/usr/bin/gdb"
      }
  ],
  "version": "2.0.0"
}